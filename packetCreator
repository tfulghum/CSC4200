import urllib.request, urllib.error, urllib.parse
import struct
def create_packet(**kwargs):
{
	data = struct.pack('!I',s_n)
	data += struct.pack('!I', a_n) 
	data += struct.pack('!{0}s'.format(len(padding), padding)
	data += struct.pack("!c", ack)
	data += struct.pack("!c", syn)
	data += struct.pack("!c", fin)
	data += struct.pack("{0}s".format(len(data),data)
	data += m_s.encode()
}

